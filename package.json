{
  "name": "lbs",
  "version": "1.0.0",
  "description": "LBS Project",
  "main": "app.js",
  "scripts": {
    "start": "npm-run-all --parallel sass:watch js:watch",
    "sass:compile": "sass app.scss dist/app.css",
    "sass:watch": "sass --watch app.scss:dist/app.css",
    "js:watch": "node --watch app.js",
    "build": "npm run sass:compile",
    "git:push": "node scripts/git-push.js",
    "gp": "node scripts/git-push.js",
    "deploy": "npm run build && npm run git:push",
    "pages:deploy": "npm run build && gh-pages -d dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "gh-pages": "^6.1.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.94.0"
  }
}

