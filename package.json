{
  "name": "lbs",
  "version": "1.0.0",
  "description": "LBS Project",
  "main": "app.js",
  "scripts": {
    "start": "npm-run-all --parallel sass:watch js:watch",
    "sass:compile": "sass app.scss dist/app.css",
    "sass:watch": "sass --watch app.scss:dist/app.css",
    "js:compile": "esbuild app.js --bundle --outfile=dist/app.js --format=iife",
    "js:watch": "esbuild app.js --bundle --outfile=dist/app.js --format=iife --watch",
    "build": "npm run sass:compile && npm run js:compile",
    "git:push": "node scripts/git-push.js",
    "gp": "node scripts/git-push.js",
    "deploy": "npm run build && npm run git:push",
    "pages:deploy": "npm run build && gh-pages -d dist",
    "deploy:fast": "npm run build && npm run git:push && echo 'âœ… Deployed! CSS available at: https://cdn.jsdelivr.net/gh/carlosgnotario/lbs@main/dist/app.css'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "gsap": "^3.12.5",
    "lenis": "^1.3.17",
    "rough-notation": "^0.5.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.22",
    "esbuild": "^0.24.0",
    "gh-pages": "^6.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "sass": "^1.94.0",
    "tailwindcss": "^4.1.17"
  }
}
